<h1>Form</h1>

<%= form_for @song do |f| %>
 <ol>
   <li>
     <%= f.text_field :title %>
   </li>
   <li>
     <%= f.check_box :released %>
   </li>
   <li>
     <%= f.number_field :release_year %>
   </li>
   <li>
     <%= f.text_field :artist_name %>
   </li>
   <li>
     <%= f.text_field :genre %>
   </li>
 </ol>
      <%= f.submit %>
<% end %>